<Window x:Class="VncMatrix.Views.VncWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VncMatrix.Views"
        xmlns:wpf="clr-namespace:VncDotnet.WPF;assembly=VncDotnet.WPF"
        mc:Ignorable="d"
        Title="VncWindow" Height="450" Width="800"
        WindowStyle="None" ResizeMode="NoResize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <wpf:VncDotnetControl x:Name="Vnc" Focusable="False" />
    </Grid>
</Window>
